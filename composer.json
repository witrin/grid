{
	"name" : "typo3/cms-grid",
	"type" : "typo3-cms-extension",
	"description" : "Grid component for the TYPO3 backend.",
	"homepage" : "https://typo3.org",
	"license" : "GPL-2.0+",
	"require" : {
		"typo3/cms-core" : "9.1.*@dev",
		"typo3/cms-backend" : "9.1.*@dev"
	},
	"replace" : {
		"grid" : "*"
	},
	"extra" : {
		"typo3/cms" : {
			"cms-package-dir" : "{$vendor-dir}/typo3/cms"
		}
	},
	"autoload" : {
		"psr-4" : {
			"TYPO3\\CMS\\Grid\\" : "Classes/"
		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"TYPO3\\CMS\\Grid\\Tests\\" : "Tests/"
		}
	}
}